}

.border-top-row {
  border-top: #ccc solid 2rpx;
}

.line {
  width: 100%;
  height: 2rpx;
  background: #aaa;
}

.hugebox {
  width: 750rpx;
  height: 1000rpx;
  background-color: #b9f8b9;
}


.bottombutton{
  width: 750rpx;
  height: 100rpx;
}

.btn1 {
  width: 60%;
  margin-top: 20rpx;
  background-color: #e6e6e6;
  color: green;
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.btnImg {
  margin-right: 8rpx;
  width: 50rpx;
  height: 50rpx;
}

.btn1::after {
  border-radius: 98rpx;
  border: 0; 
}


.btn2 {
  width: 60%;
  margin-top: 20rpx;
  background-color: #e6e6e6;
  color: rgba(255, 0, 0, 0.726);
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.btn2::after {
  border-radius: 98rpx;
  border: 0; 
}

.floatingborder{
  width: 650rpx;
  position: absolute;
  top: 140rpx;
  left: 50rpx;
  background-color: #e2f7e2;
  color: green;
  border-radius: 30rpx;
  display: table-cell;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.greenbox{
  width: 750rpx;
