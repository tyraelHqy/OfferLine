.box {
  width: 750rpx;
  height: 30rpx;
  overflow-x: hidden;
  background-color: #b9f8b9;
}

.greenbox{
  width: 750rpx;
  height: 30rpx;
  overflow-x: hidden;
  background-color: green;
}

.lightgreenbox{
  width: 750rpx;
  height: 10rpx;
  overflow-x: hidden;
  background-color: #e2f7e2;
}

.whitebox {
  width: 750rpx;
  height: 10rpx;
  overflow-x: hidden;
  background-color: white;
}

.cell-title {
  color: green !important;
  font-size: 39rpx !important;
  text-align: center;
  font-weight: bold;
}

.cell-white-title{
  color: white !important;
  background-color: green;
  font-size: 40rpx !important;
  text-align: center;
  font-weight: bold;
}

.righttext {
  color: black !important;
  font-size: 30rpx !important;
  text-align: right;
}

.cell-value {
  width: 25%;
  color: black !important;
  font-size: 28rpx !important;
  text-align: center;
}

.border-top-row {
  border-top: #ccc solid 2rpx;
}

.line {
  width: 100%;
  height: 2rpx;
  background: #aaa;
}

.hugebox {
  width: 750rpx;
  height: 1000rpx;
  background-color: #b9f8b9;
}

.floatingborder{
  display: flex;
  width: 100%;
  height: 100%;
  top: 150rpx;
  position: relative;
  overflow-x: hidden;
  background-color: white;
  color: green;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.floatingText{
  display: flex;
  width: 100%;
  height: 100%;
  top: 100rpx;
  align-items: center;
  justify-content: center;  
  position: relative;
  overflow-x: hidden;
  background-color: white;
  color: black;
  border-radius: 10rpx;
}

.widthView{
  width:750rpx;
  padding: 10rpx;
  position: relative;
  vertical-align: middle;
  height:100rpx;
  background-color: #e2f7e2;
  color: black;
  border-radius: 40rpx;
  display:table-cell;
  flex-direction: row;
  justify-content: center;
}

.btnImg {
  width: 50rpx;
  height: 50rpx;
}
/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #aaa;
}

.userinfo-avatar {
  overflow: hidden;
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.usermotto {
  margin-top: 100px;
}

